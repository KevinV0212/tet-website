@{
    ViewData["Title"] = "Tet 2024";
}
<div class="banner-top">
    <img src="~/img/top-banner-desktop-v2.jpg" alt="top banner" height="100%" width="auto">
</div>
<div class="content">
    <section id="summary" class="summary">
        <div class="card">
            <h1 class="card-header">Tết 2024 - Year of the Dragon</h1>
            <div class="card-inner">Come join us at our 30th Annual Vietnamese lunar new year festival. This year, we are celebrating the year of the dragon, the luckiest of all Zodiac animals. Those born in the year of the dragon possess remarkable qualities such as  cleverness, charm, eloquence, and even wealth.</div>
        </div>
    </section>
    <div class="time-ven">
        <section id="schedule" class="schedule">
            <h1 class="section-heading">Schedule</h1>
            <div class="card">
                <div class="card-inner">
                    <div class="btn-group">
                        <button id="sat_btn" class="active">Sat</button>
                        <button id="sun_btn">Sun</button>
                    </div>
                    <ul id="event_list" class="event_list">
                    </ul>
                </div>
            </div>
        </section>
        <section id="venue" class="venue">
            <h1 class="section-heading">Venue</h1>
            <div class="card">
                <div class="card-inner">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d7008.937086479451!2d-81.44029442512888!3d28.55568987570673!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x88e7799023a6a435%3A0xb21922b1d71bb593!2sCentral%20Florida%20Fair!5e0!3m2!1sen!2sus!4v1728420883387!5m2!1sen!2sus" width="100%" height="250" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                </div>
            </div>
        </section>
    </div>
    <section id="raffle" class="raffle">
        <h1 class="section-heading">Raffle</h1>
        <div class="card">
            <div class="card-inner">
                <h3 class="card-header">Grand Prize</h3>
                <div class="swiper">
                    <!-- Additional required wrapper -->
                    <div class="swiper-wrapper">
                        <!-- Slides -->
                        <div class="swiper-slide">
                            <img src="~/img/rav4/front.png" alt="">
                            <div class="caption">Grand Prize: 2024 Toyota Rav4</div>
                        </div>
                        <div class="swiper-slide">
                            <img src="~/img/rav4/side.png" alt="">
                            <p class="caption">Grand Prize: 2024 Toyota Rav4</p>
                        </div>
                        <div class="swiper-slide">
                            <img src="~/img/rav4/back.png" alt="">
                            <p class="caption">Grand Prize: 2024 Toyota Rav4</p>
                        </div>
                    </div>
                    <!-- If we need pagination -->
                    <div class="swiper-pagination"></div>

                    <!-- If we need navigation buttons -->
                    <div class="swiper-button-prev"></div>
                    <div class="swiper-button-next"></div>

                    <!-- If we need scrollbar -->
                </div>
                <hr>
                <h3 class="card-header">All Prizes</h3>
                <div class="swiper">
                    <!-- Additional required wrapper -->
                    <div class="swiper-wrapper">
                        <!-- Slides -->
                        <div class="swiper-slide">
                            <img src="~/img/rav4/front.png" alt="">
                            <p class="caption">1st Place: 2024 Toyota Rav4</p>
                        </div>
                        <div class="swiper-slide">
                            <img src="~/img/gift-card.png" alt="">
                            <p class="caption">2nd Place: $1000 Gift Card</p>
                        </div>
                        <div class="swiper-slide">
                            <img src="~/img/gift-card.png" alt="">
                            <p class="caption">3rd Place: $500 Gift Card</p>
                        </div>
                    </div>
                    <!-- If we need pagination -->
                    <div class="swiper-pagination"></div>

                    <!-- If we need navigation buttons -->
                    <div class="swiper-button-prev"></div>
                    <div class="swiper-button-next"></div>

                    <!-- If we need scrollbar -->
                </div>
            </div>
        </div>
    </section>
    <section id="contact" class="contact">
